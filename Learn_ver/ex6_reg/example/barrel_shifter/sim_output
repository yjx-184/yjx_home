#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5cfd0dac8dc0 .scope module, "tb_barrel_shifter" "tb_barrel_shifter" 2 1;
 .timescale 0 0;
v0x5cfd0dadec90_0 .var "AL", 0 0;
v0x5cfd0daded50_0 .var "LR", 0 0;
v0x5cfd0dadedf0_0 .var "din", 7 0;
v0x5cfd0dadeef0_0 .net "dout", 7 0, v0x5cfd0dade9e0_0;  1 drivers
v0x5cfd0dadefc0_0 .var "shamt", 2 0;
S_0x5cfd0dac8f50 .scope module, "uut" "barrel_shifter" 2 10, 3 1 0, S_0x5cfd0dac8dc0;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "din";
    .port_info 1 /INPUT 3 "shamt";
    .port_info 2 /INPUT 1 "LR";
    .port_info 3 /INPUT 1 "AL";
    .port_info 4 /OUTPUT 8 "dout";
v0x5cfd0da8f7b0_0 .net "AL", 0 0, v0x5cfd0dadec90_0;  1 drivers
v0x5cfd0da8fba0_0 .net "LR", 0 0, v0x5cfd0daded50_0;  1 drivers
v0x5cfd0dade920_0 .net "din", 7 0, v0x5cfd0dadedf0_0;  1 drivers
v0x5cfd0dade9e0_0 .var "dout", 7 0;
v0x5cfd0dadeac0_0 .net "shamt", 2 0, v0x5cfd0dadefc0_0;  1 drivers
E_0x5cfd0dac7170 .event edge, v0x5cfd0da8fba0_0, v0x5cfd0dade920_0, v0x5cfd0dadeac0_0, v0x5cfd0da8f7b0_0;
    .scope S_0x5cfd0dac8f50;
T_0 ;
    %wait E_0x5cfd0dac7170;
    %load/vec4 v0x5cfd0da8fba0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x5cfd0dade920_0;
    %ix/getv 4, v0x5cfd0dadeac0_0;
    %shiftl 4;
    %store/vec4 v0x5cfd0dade9e0_0, 0, 8;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5cfd0da8f7b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5cfd0dade920_0;
    %ix/getv 4, v0x5cfd0dadeac0_0;
    %shiftr/s 4;
    %store/vec4 v0x5cfd0dade9e0_0, 0, 8;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5cfd0dade920_0;
    %ix/getv 4, v0x5cfd0dadeac0_0;
    %shiftr 4;
    %store/vec4 v0x5cfd0dade9e0_0, 0, 8;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5cfd0dac8dc0;
T_1 ;
    %vpi_call 2 20 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5cfd0dac8dc0 {0 0 0};
    %vpi_call 2 22 "$display", "Dumping waveforms to dump.vcd..." {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5cfd0dac8dc0;
T_2 ;
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x5cfd0dadedf0_0, 0, 8;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x5cfd0dadefc0_0, 0, 3;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5cfd0daded50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cfd0dadec90_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cfd0daded50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cfd0dadec90_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5cfd0dadec90_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x5cfd0dadefc0_0, 0, 3;
    %delay 10, 0;
    %pushi/vec4 7, 0, 3;
    %store/vec4 v0x5cfd0dadefc0_0, 0, 3;
    %delay 10, 0;
    %vpi_call 2 50 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5cfd0dac8dc0;
T_3 ;
    %vpi_call 2 55 "$monitor", "Time = %0t | din = %b | shamt = %d | L_R = %b | A_L = %b | dout = %b", $time, v0x5cfd0dadedf0_0, v0x5cfd0dadefc0_0, v0x5cfd0daded50_0, v0x5cfd0dadec90_0, v0x5cfd0dadeef0_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_barrel_shifter.v";
    "barrel_shifter.v";
