#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5e9c0dd8fdc0 .scope module, "tb_shifter32" "tb_shifter32" 2 1;
 .timescale 0 0;
v0x5e9c0dda70f0_0 .var "AL", 0 0;
v0x5e9c0dda71b0_0 .var "LR", 0 0;
v0x5e9c0dda7250_0 .var "din", 31 0;
v0x5e9c0dda7320_0 .net "dout", 31 0, v0x5e9c0dda6e40_0;  1 drivers
v0x5e9c0dda73f0_0 .var "shamt", 4 0;
S_0x5e9c0dd8ff50 .scope module, "uut" "shifter32" 2 10, 3 1 0, S_0x5e9c0dd8fdc0;
 .timescale 0 0;
    .port_info 0 /INPUT 32 "din";
    .port_info 1 /INPUT 5 "shamt";
    .port_info 2 /INPUT 1 "LR";
    .port_info 3 /INPUT 1 "AL";
    .port_info 4 /OUTPUT 32 "dout";
v0x5e9c0dd567b0_0 .net "AL", 0 0, v0x5e9c0dda70f0_0;  1 drivers
v0x5e9c0dd56ba0_0 .net "LR", 0 0, v0x5e9c0dda71b0_0;  1 drivers
v0x5e9c0dda6d80_0 .net "din", 31 0, v0x5e9c0dda7250_0;  1 drivers
v0x5e9c0dda6e40_0 .var "dout", 31 0;
v0x5e9c0dda6f20_0 .net "shamt", 4 0, v0x5e9c0dda73f0_0;  1 drivers
E_0x5e9c0dd8e170 .event edge, v0x5e9c0dd56ba0_0, v0x5e9c0dda6d80_0, v0x5e9c0dda6f20_0, v0x5e9c0dd567b0_0;
    .scope S_0x5e9c0dd8ff50;
T_0 ;
    %wait E_0x5e9c0dd8e170;
    %load/vec4 v0x5e9c0dd56ba0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x5e9c0dda6d80_0;
    %ix/getv 4, v0x5e9c0dda6f20_0;
    %shiftl 4;
    %store/vec4 v0x5e9c0dda6e40_0, 0, 32;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5e9c0dd567b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5e9c0dda6d80_0;
    %ix/getv 4, v0x5e9c0dda6f20_0;
    %shiftr/s 4;
    %store/vec4 v0x5e9c0dda6e40_0, 0, 32;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5e9c0dda6d80_0;
    %ix/getv 4, v0x5e9c0dda6f20_0;
    %shiftr 4;
    %store/vec4 v0x5e9c0dda6e40_0, 0, 32;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5e9c0dd8fdc0;
T_1 ;
    %vpi_call 2 20 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5e9c0dd8fdc0 {0 0 0};
    %vpi_call 2 22 "$display", "Dumping waveforms to dump.vcd..." {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5e9c0dd8fdc0;
T_2 ;
    %pushi/vec4 2863311530, 0, 32;
    %store/vec4 v0x5e9c0dda7250_0, 0, 32;
    %pushi/vec4 2, 0, 5;
    %store/vec4 v0x5e9c0dda73f0_0, 0, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e9c0dda71b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e9c0dda70f0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 2863311530, 0, 32;
    %store/vec4 v0x5e9c0dda7250_0, 0, 32;
    %pushi/vec4 2, 0, 5;
    %store/vec4 v0x5e9c0dda73f0_0, 0, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e9c0dda71b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e9c0dda70f0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 2863311530, 0, 32;
    %store/vec4 v0x5e9c0dda7250_0, 0, 32;
    %pushi/vec4 2, 0, 5;
    %store/vec4 v0x5e9c0dda73f0_0, 0, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e9c0dda71b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e9c0dda70f0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 4294967290, 0, 32;
    %store/vec4 v0x5e9c0dda7250_0, 0, 32;
    %pushi/vec4 2, 0, 5;
    %store/vec4 v0x5e9c0dda73f0_0, 0, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e9c0dda71b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e9c0dda70f0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 56 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5e9c0dd8fdc0;
T_3 ;
    %vpi_call 2 61 "$monitor", "Time = %0t | din = %b | shamt = %d | LR = %b | AL = %b | dout = %b", $time, v0x5e9c0dda7250_0, v0x5e9c0dda73f0_0, v0x5e9c0dda71b0_0, v0x5e9c0dda70f0_0, v0x5e9c0dda7320_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
