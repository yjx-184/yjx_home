#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x6427e07103b0 .scope module, "Instr_Mem_tb" "Instr_Mem_tb" 2 3;
 .timescale -9 -12;
v0x6427e0723f60_0 .net "Instr", 31 0, v0x6427e06fe120_0;  1 drivers
v0x6427e0724050_0 .var "Rdaddr", 31 0;
v0x6427e0724120_0 .var "Rdclk", 0 0;
S_0x6427e0710540 .scope module, "uut" "Instr_Mem" 2 11, 3 1 0, S_0x6427e07103b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "Rdclk";
    .port_info 1 /INPUT 32 "Rdaddr";
    .port_info 2 /OUTPUT 32 "Instr";
P_0x6427e06fb060 .param/l "MEM_DEPTH" 0 3 8, +C4<00000000000000000000000000000001>;
P_0x6427e06fb0a0 .param/l "MEM_WIDTH" 0 3 9, +C4<00000000000000000000000000100000>;
v0x6427e06fe120_0 .var "Instr", 31 0;
v0x6427e0723c50_0 .net "Rdaddr", 31 0, v0x6427e0724050_0;  1 drivers
v0x6427e0723d30_0 .net "Rdclk", 0 0, v0x6427e0724120_0;  1 drivers
v0x6427e0723e00 .array "mem_array", 0 0, 31 0;
E_0x6427e06fd860 .event negedge, v0x6427e0723d30_0;
    .scope S_0x6427e0710540;
T_0 ;
    %vpi_call 3 16 "$readmemh", "instr_mem.hex", v0x6427e0723e00 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x6427e0710540;
T_1 ;
    %wait E_0x6427e06fd860;
    %load/vec4 v0x6427e0723c50_0;
    %parti/s 30, 2, 3;
    %ix/vec4 4;
    %load/vec4a v0x6427e0723e00, 4;
    %assign/vec4 v0x6427e06fe120_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x6427e07103b0;
T_2 ;
    %load/vec4 v0x6427e0724120_0;
    %inv;
    %store/vec4 v0x6427e0724120_0, 0, 1;
    %delay 5000, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x6427e07103b0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6427e0724120_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x6427e0724050_0, 0, 32;
    %vpi_call 2 30 "$monitor", "At time %t, Rdaddr = %d, Instr = %h", $time, v0x6427e0724050_0, v0x6427e0723f60_0 {0 0 0};
    %delay 10000, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x6427e0724050_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x6427e0724050_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x6427e0724050_0, 0, 32;
    %delay 10000, 0;
    %vpi_call 2 43 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x6427e07103b0;
T_4 ;
    %vpi_call 2 48 "$readmemh", "instr_mem.hex", v0x6427e0723e00 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
